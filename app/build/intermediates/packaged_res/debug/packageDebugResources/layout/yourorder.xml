<?xml version="1.0" encoding="utf-8"?>

<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container_your_order"
    android:layout_width="match_parent"
    android:layout_height="844dp">

    <ImageView
        android:id="@+id/img_ellipse"
        android:layout_width="714dp"
        android:layout_height="752dp"
        android:layout_marginLeft="-169dp"
        android:layout_marginTop="-376dp"
        android:layout_gravity="top|left"
        android:background="@drawable/rectangle_1"
        android:scaleType="centerInside" />

    <ImageView
        android:id="@+id/img_rectangle"
        android:layout_width="291dp"
        android:layout_height="452dp"
        android:layout_marginLeft="56dp"
        android:layout_marginTop="125dp"
        android:layout_gravity="top|left"
        android:background="@drawable/image_rectangle"
        android:scaleType="centerInside" />

    <TextView
        android:id="@+id/text_no_order_found"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginLeft="103dp"
        android:layout_marginTop="457.5dp"
        android:layout_marginBottom="-0.5dp"
        android:layout_gravity="top|left"
        android:gravity="left|center_vertical"
        android:includeFontPadding="false"
        android:maxLines="1"
        android:singleLine="true"
        android:text="NO ORDER FOUND"
        android:textColor="#FF000000"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/text_no_order_yet"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_marginLeft="106dp"
        android:layout_marginTop="495dp"
        android:layout_gravity="top|left"
        android:gravity="center_horizontal|top"
        android:includeFontPadding="false"
        android:lineHeight="14.52dp"
        android:text="Looks like you havenâ€™t made\nyour order yet"
        android:textColor="#FF000000"
        android:textSize="12dp" />

    <ImageView
        android:id="@+id/img_group"
        android:layout_width="208.73dp"
        android:layout_height="164dp"
        android:layout_marginLeft="85dp"
        android:layout_marginTop="219dp"
        android:layout_gravity="top|left"
        android:scaleType="centerInside"
        />

    <FrameLayout
        android:id="@+id/container_group"
        android:layout_width="match_parent"
        android:layout_height="99dp"
        android:layout_gravity="top">

        <ImageView
            android:id="@+id/img_group_rectangle"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/image_rectangle"
            android:scaleType="centerCrop" />

        <FrameLayout
            android:id="@+id/container_status_bar"
            android:layout_width="match_parent"
            android:layout_height="23.02dp"
            android:layout_marginLeft="35.62dp"
            android:layout_marginRight="16.44dp"
            android:layout_marginTop="12.66dp"
            android:layout_gravity="top">


            <ImageView
                android:id="@+id/img_sim"
                android:layout_width="20.82dp"
                android:layout_height="13.81dp"
                android:layout_marginLeft="288.25dp"
                android:layout_marginRight="49.87dp"
                android:layout_marginTop="5.76dp"
                android:layout_marginBottom="3.45dp"
                android:scaleType="centerInside" />




        </FrameLayout>

        <TextView
            android:id="@+id/text_recent_orders"
            android:layout_width="match_parent"
            android:layout_height="25.33dp"
            android:layout_marginLeft="118.37dp"
            android:layout_marginRight="117.27dp"
            android:layout_marginTop="57.56dp"
            android:layout_marginBottom="16.12dp"
            android:gravity="left|center_vertical"
            android:includeFontPadding="false"
            android:maxLines="1"
            android:singleLine="true"
            android:text="Your Recent Orders"
            android:textColor="#FF000000"
            android:textSize="18dp" />

        <ImageView
            android:id="@+id/img_vector"
            android:layout_width="24.66dp"
            android:layout_height="25.9dp"
            android:layout_marginLeft="16.17dp"
            android:layout_marginRight="370.17dp"
            android:layout_marginTop="61.88dp"
            android:layout_marginBottom="11.22dp"
            android:scaleType="centerInside" />

        <ImageView
            android:id="@+id/img_vector1"
            android:layout_width="6.85dp"
            android:layout_height="14.39dp"
            android:layout_marginLeft="24.39dp"
            android:layout_marginRight="379.76dp"
            android:layout_marginTop="67.63dp"
            android:layout_marginBottom="16.98dp"
            android:scaleType="centerInside"
            android:src="@drawable/baseline_arrow_back_ios_24" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/container_login"
        android:layout_width="336dp"
        android:layout_height="45dp"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="621dp"
        android:layout_gravity="top|left">

        <Button
            android:id="@+id/img_login_box"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:backgroundTint="#FFD580"
            android:scaleType="centerCrop" />

        <TextView
            android:id="@+id/text_start_order"
            android:layout_width="match_parent"
            android:layout_height="18dp"
            android:layout_marginLeft="98dp"
            android:layout_marginRight="97dp"
            android:layout_marginTop="13.5dp"
            android:layout_marginBottom="13.5dp"
            android:gravity="left|center_vertical"
            android:includeFontPadding="false"
            android:maxLines="1"
            android:singleLine="true"
            android:text="START YOUR ORDER"
            android:textColor="#000000"
            android:textSize="14dp" />
    </FrameLayout>

    <ImageView
        android:id="@+id/container_line"
        android:layout_width="133dp"
        android:layout_height="4.4dp"
        android:layout_marginRight="121dp"
        android:layout_marginBottom="2.6dp"
        android:layout_gravity="bottom|right"
        android:scaleType="centerInside" />
</FrameLayout>